package com.pojo;

public class Manager extends Employee 
{
	private String dpt;
	private int inct;
	
	public Manager() 
	{
		// TODO Auto-generated constructor stub
		dpt = "nothing";
		inct = 0;
		
	}

	public String getDpt() {
		return dpt;
	}

	public void setDpt(String dpt) {
		this.dpt = dpt;
	}

	public int getInct() {
		return inct;
	}

	public void setInct(int inct) {
		this.inct = inct;
	}
	
	public void display()
	{
		System.out.println("Department = "+ dpt + "\t" + "incentives = "+inct );
		super.display();
	}
	
	
}
