
public class DemoJagged {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [][] values = new int[3][];
		values[0] = new int [4];
		values[0][0] = 11;
		values[0][1] = 12;
		values[0][2] = 13;
		values[0][3] = 14;
		
		values[1] = new int [2];
		values[1][0] = 21;
		values[1][1] = 22;
		
		values[2] = new int[3];
		values[2][0] = 31;
		values[2][1] = 32;
		values[2][2] = 33;
		
		for(int[] arr : values)
		{
			for(int v : arr)
			{
				System.out.print(v+ "\t");
			}
			
			System.out.println();
		}

	}

}
